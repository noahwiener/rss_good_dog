<div id="navbar" class="navbar-collapse collapse noah-nav">
  <ul class="nav navbar-nav">
    <%= link_to '<img src="/New-York-Times-Logo.jpg" height="60px">'.html_safe,
                        articles_path %>
  </ul>
  <ul class="nav navbar-nav navbar-right">
    <li>
      <% if current_user %>
        <span>Hi, <%= current_user.user_name %></span>
        <%= link_to "Favorite Articles", user_path(current_user) %>
        <%= link_to "Sign Out", session_url, :method => :delete %>
      <% else %>
        <%= link_to "Sign In", new_session_url %> <%= link_to "Sign Up", new_user_url %>
      <% end %>
    </li>
  </ul>
</div>
